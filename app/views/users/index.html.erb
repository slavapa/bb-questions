<%= render partial: 'shared/header_index', 
	locals:{ title: t('.list'),  path: new_user_path, search: @search } %>

<%= render partial: 'user_search', 
	locals:{ search: @search} %>	

<div class="paginate-cont-top">
  <%= will_paginate %>
</div>	

<table class='table-bordered tabletable-hover table-index'>
  <thead>
    <tr>
      <th class='td_left '></th>
      <th class='td_holder medium-size-td  module-link'><%=  t(:name) %></th>
      <th class='td_holder medium-size-td'><%= t('activerecord.attributes.user.family_name')%></th>
      <th class='td_holder mm-size-td'><%=  t('activerecord.attributes.user.email') %></th>
    </tr>
  </thead>

  <tbody  class="tbody_events">
  	<%= render @users %>
  </tbody>
</table>

<div class="paginate-cont-bottom">
  <%= will_paginate %>
</div>